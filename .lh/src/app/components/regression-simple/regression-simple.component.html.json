            "date": 1678127491199,
            "name": "Commit-0",
            "content": "<section *ngIf=\"dataFormatted.length\">\n    <div class=\"mb-3 row align-items-center calc\">\n      <pre>Calcula un valor para la variable 2</pre>\n      <div class=\"col-11 align-items-center\">\n        <div class=\" row align-items-center\">\n          <div class=\"col-4\">\n            <pre for=\"\" class=\"form-label\"> Variable 1</pre>\n            <input type=\"number\" class=\"form-control\" name=\"inputName\" [(ngModel)]=\"CalcColumnX\" placeholder=\"00\" (blur)=\"calculateYSimple()\">\n          </div>\n          <div class=\"col-4\">\n            <pre for=\"\" class=\"form-label\"> Variable 2</pre>\n            <input type=\"number\" class=\"form-control\" name=\"inputName\" [(ngModel)]=\"CalcColumnY\"  [readOnly]=\"true\" placeholder=\"00\">\n          </div>\n        </div>\n      </div>\n     \n    </div>\n  </section>\n<section>\n    <div class=\"table-responsive\">\n        <table class=\"table table-striped-columns\n        table-hover\t\n        table-borderless\n        table-primary\n        align-middle\">\n          <thead class=\"table-light\">\n            <caption>Solución</caption>\n            <tr>\n              <th>A</th>\n              <th>B</th>\n              <th>R</th>\n            </tr>\n          </thead>\n          <tbody class=\"table-group-divider\">\n  \n            <tr class=\"table-primary\">\n  \n              <td scope=\"row\">{{a| number}}</td>\n              <td>{{b | number}}</td> <!-- (D26-(C31*C26))/22 -->\n              <td>{{r | number}}</td>\n  \n  \n            </tr>\n  \n  \n          </tbody>\n          <tfoot>\n  \n          </tfoot>\n        </table>\n      </div>\n  \n      <div class=\"table-responsive\">\n        <table class=\"table table-striped-columns\n        table-hover\t\n        table-borderless\n        table-primary\n        align-middle\">\n          <thead class=\"table-light\">\n            <caption>Valores Sumatorios y totales</caption>\n            <tr>\n              <th>∑ X</th>\n              <th>∑ Y</th>\n              <th>∑ Ẍ</th>\n              <th>∑ Ÿ</th>\n              <th>∑(X⋅Y)</th>\n              <th>∑(x²)</th>\n              <th>∑(X-Ẍ)</th>\n              <th>∑(Y-Ÿ)</th>\n              <th>∑((X-Ẍ)²)</th>\n              <th>∑((Y-Ÿ)²)</th>\n              <th>∑((X-Ẍ)⋅(Y-Ÿ))</th>\n              <th>(∑ X)²</th>\n            </tr>\n          </thead>\n          <tbody class=\"table-group-divider\">\n  \n            <tr class=\"table-primary\">\n              <td scope=\"row\">{{sumatoria_variable1 | number}}</td>\n              <!--<th>∑ X</th> -->\n              <td>{{sumatoria_variable2 | number}}</td>\n              <!--<th>∑ Y</th> -->\n              <td>{{promedio_1 | number}}</td>\n              <!--<th>∑ Ẍ</th> -->\n              <td>{{promedio_2 | number}}</td>\n              <!--<th>∑ Ÿ</th> -->\n              <td>{{sumatoria_variable1_X_variable2 | number}}</td>\n              <!--<th>∑(X⋅Y)</th> -->\n              <td>{{sumatoria_cuadrado_variable1 | number}}</td>\n              <!--<th>∑(x²)</th> -->\n              <td>{{sumatoria_variable1_menos_proVariable1 | number}}</td>\n              <!--<th>∑(X-Ẍ)</th> -->\n              <td>{{sumatoria_variable2_menos_proVariable2 | number}}</td>\n              <!--<th>∑(Y-Ÿ)</th> -->\n              <td>{{sumatoria_cuadrado_variable1_menos_proVariable1 | number}}</td>\n              <!--<th>∑((X-Ẍ)²)</th> -->\n              <td>{{sumatoria_cuadrado_variable2_menos_proVariable2 | number}}</td>\n              <!--<th>∑((Y-Ÿ)²)</th> -->\n              <td>{{sumatoria_variable1_menos_proVariable1_X_variable2_menos_proVariable2 | number}}</td>\n              <!--<th>∑((X-Ẍ)⋅(Y-Ÿ))</th> -->\n              <td>{{sumatoria_variable1 * sumatoria_variable1 | number}}</td><!--             <th>(∑ X)²</th> -->\n  \n            </tr>\n  \n          </tbody>\n          <tfoot>\n  \n          </tfoot>\n        </table>\n      </div>\n  \n      <div class=\"table-responsive\">\n        <table class=\"table table-striped-columns\n        table-hover\t\n        table-borderless\n        table-primary\n        align-middle\">\n          <thead class=\"table-light\">\n            <caption>Regresión</caption>\n            <tr>\n              <th>Año</th>\n              <th>X</th>\n              <th>Y</th>\n              <th>X⋅Y</th>\n              <th>X²</th>\n              <th>X-Ẍ</th>\n              <th>Y-Ÿ</th>\n              <th>(X-Ẍ)²</th>\n              <th>(Y-Ÿ)²</th>\n              <th>(X-Ẍ)⋅(Y-Ÿ)</th>\n            </tr>\n          </thead>\n          <tbody class=\"table-group-divider\">\n  \n            <tr class=\"table-primary\" *ngFor=\"let item of dataFormatted\">\n              <td scope=\"row\">{{item.variable_time}}</td>\n              <td>{{item.variable1 | number }}</td>\n              <td>{{item.variable2 | number }}</td>\n              <td>{{item.variable1_X_variable2 | number }}</td>\n              <td>{{item.cuadrado_variable1 | number }}</td>\n              <td>{{item.variable1_menos_proVariable1 | number }}</td>\n              <td>{{item.variable2_menos_proVariable2 | number }}</td>\n              <td>{{item.cuadrado_variable1_menos_proVariable1 | number }}</td>\n              <td>{{item.cuadrado_variable2_menos_proVariable2 | number}}</td>\n              <td>{{item.variable1_menos_proVariable1_X_variable2_menos_proVariable2 | number}}</td>\n            </tr>\n  \n          </tbody>\n          <tfoot>\n  \n          </tfoot>\n        </table>\n      </div>\n  \n</section>"
        }
